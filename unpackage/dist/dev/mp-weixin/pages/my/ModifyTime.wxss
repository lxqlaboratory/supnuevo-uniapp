/* 回到顶部的按钮 */
.totop {
	z-index: 9990;
	position: fixed !important;
	/* 避免编译到H5,在多mescroll中定位失效 */
	right: 20rpx;
	bottom: 120rpx;
	width: 72rpx;
	height: 72rpx;
	border-radius: 50%;
	opacity: 1;
}
.fade-in {
	-webkit-animation: mescrollFadeIn .3s linear forwards;
	animation: mescrollFadeIn .3s linear forwards;
}


